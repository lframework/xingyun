<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lframework.xingyun.template.inner.mappers.system.SysOpenDomainMapper">

    <sql id="SysOpenDomain_sql">
        SELECT
            *
        FROM sys_open_domain
    </sql>

    <select id="query" resultType="com.lframework.xingyun.template.inner.entity.SysOpenDomain">
        <include refid="SysOpenDomain_sql"/>
        <where>
            <if test="vo != null">
                <if test="vo.id != null and vo.id != ''">
                    AND id = #{vo.id}
                </if>
                <if test="vo.name != null and vo.name != ''">
                    AND name LIKE CONCAT('%', #{vo.name}, '%')
                </if>
                <if test="vo.available != null">
                    AND available = #{vo.available}
                </if>
            </if>
        </where>
    </select>
    <select id="selector" resultType="com.lframework.xingyun.template.inner.entity.SysOpenDomain">
        <include refid="SysOpenDomain_sql"/>
        <where>
            <if test="vo != null">
                <if test="vo.id != null and vo.id != ''">
                    AND id = #{vo.id}
                </if>
                <if test="vo.name != null and vo.name != ''">
                    AND name LIKE CONCAT('%', #{vo.name}, '%')
                </if>
                <if test="vo.available != null">
                    AND available = #{vo.available}
                </if>
            </if>
        </where>
    </select>
</mapper>
